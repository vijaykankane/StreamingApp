apiVersion: v1
kind: Service
metadata:
  name: frontend-lb
  namespace: shopnow-demo
  annotations:
    # Example: choose nginx LB type; provider will create LB automatically
    service.beta.kubernetes.io/aws-load-balancer-backend-protocol: http
spec:
  type: LoadBalancer
  selector:
    app: frontend
  ports:
  - port: 80
    targetPort: 80

